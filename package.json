{
  "name": "kavanest-store",
  "version": "1.0.0",
  "main": "dist/index.ts",
  "repository": "https://github.com/InvisiBug/kavanest-store.git",
  "author": "InvisiBug <matthewjkavanagh@gmail.com>",
  "license": "MIT",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec \"ts-node \" ./src/index.ts",
    "start": "ts-node ./src/index.ts",
    "build": "tsc && cp ./src/devices.yaml ./dist",
    "start-container": "./bin/start-container.sh",
    "up": "clear && ./bin/start-container.sh",
    "down": "docker-compose down",
    "logs": "docker logs kavanest-scraper -f",
    "push": "./bin/push.sh",
    "deploy": "./bin/deploy.sh"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.4",
    "@types/node": "^16.11.6",
    "@types/ws": "^8.2.0",
    "config": "^3.3.6",
    "dotenv": "^10.0.0",
    "js-yaml": "^4.1.0",
    "mongoose": "^6.0.12",
    "mqtt": "^4.2.8",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
